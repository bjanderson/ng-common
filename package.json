{
  "name": "ng-common-lib",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run build",
    "build": "npm run build:prod --trace-warnings",
    "prebuild:prod": "npm run clean",
    "build:prod": "ng build --configuration=production && npm run copysass",
    "clean": "rimraf \"./dist*\"",
    "copysass": "cpx \"./projects/ng-common/src/**/*.scss\" dist/ng-common",
    "lint": "ng lint",
    "test": "ng test",
    "test:cov": "ng test --coverage",
    "prepush": "npm run build",
    "push": "node scripts/publish.js",
    "v": "npm --prefix ./projects/ng-common version",
    "push:major": "npm run v -- major",
    "push:minor": "npm run v -- minor",
    "push:patch": "npm run v -- patch"
  },
  "devDependencies": {
    "@angular-builders/jest": "12.1.0",
    "@angular-devkit/build-angular": "12.1.0",
    "@angular-devkit/core": "12.1.0",
    "@angular-devkit/schematics": "12.1.0",
    "@angular-eslint/builder": "12.2.0",
    "@angular-eslint/eslint-plugin": "12.2.0",
    "@angular-eslint/eslint-plugin-template": "12.2.0",
    "@angular-eslint/schematics": "12.2.0",
    "@angular-eslint/template-parser": "12.2.0",
    "@angular/animations": "12.1.0",
    "@angular/cdk": "12.1.0",
    "@angular/cli": "12.1.0",
    "@angular/common": "12.1.0",
    "@angular/compiler": "12.1.0",
    "@angular/compiler-cli": "12.1.0",
    "@angular/core": "12.1.0",
    "@angular/forms": "12.1.0",
    "@angular/material": "12.1.0",
    "@angular/platform-browser": "12.1.0",
    "@angular/platform-browser-dynamic": "12.1.0",
    "@angular/router": "12.1.0",
    "@bjanderson/utils": "1.0.1",
    "@types/jasmine": "3.7.7",
    "@types/jest": "26.0.23",
    "@types/node": "15.12.5",
    "@typescript-eslint/eslint-plugin": "4.28.1",
    "@typescript-eslint/parser": "4.28.1",
    "core-js": "3.15.1",
    "cpx": "1.5.0",
    "eslint": "7.29.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jasmine": "4.1.2",
    "eslint-plugin-jsdoc": "35.4.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-rxjs": "3.3.4",
    "jasmine-core": "3.7.1",
    "jest": "27.0.6",
    "jest-cli": "27.0.6",
    "ng-packagr": "12.1.0",
    "prettier": "2.3.2",
    "prettier-eslint": "12.0.0",
    "rimraf": "3.0.2",
    "rxjs": "6.6.7",
