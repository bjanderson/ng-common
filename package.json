{
  "name": "@bjanderson/ng-common-lib",
  "version": "5.0.0",
  "license": "MIT",
  "publishConfig": {
    "@bjanderson:registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bjanderson/ng-common.git"
  },
  "scripts": {
    "start": "npm run build",
    "build": "npm run build:prod --trace-warnings",
    "prebuild:prod": "npm run clean",
    "build:prod": "ng build --configuration=production && npm run copysass",
    "clean": "rimraf \"./dist*\"",
    "copysass": "cpx \"./projects/ng-common/src/**/*.scss\" dist/ng-common",
    "lint": "ng lint",
    "test": "ng test",
    "test:cov": "ng test --coverage",
    "prepublish": "npm run push",
    "prepush": "npm run build",
    "push": "node scripts/publish.js",
    "v": "npm --prefix ./projects/ng-common version",
    "push:major": "npm version major && npm run v -- major",
    "push:minor": "npm version minor && npm run v -- minor",
    "push:patch": "npm version patch && npm run v -- patch"
  },
  "devDependencies": {
    "@angular-builders/jest": "16.0.0",
    "@angular-devkit/build-angular": "16.1.4",
    "@angular-devkit/core": "16.1.4",
    "@angular-devkit/schematics": "16.1.4",
    "@angular-eslint/builder": "16.1.0",
    "@angular-eslint/eslint-plugin": "16.1.0",
    "@angular-eslint/eslint-plugin-template": "16.1.0",
    "@angular-eslint/schematics": "16.1.0",
    "@angular-eslint/template-parser": "16.1.0",
    "@angular/animations": "16.1.5",
    "@angular/cdk": "16.1.5",
    "@angular/cli": "16.1.4",
    "@angular/common": "16.1.5",
    "@angular/compiler": "16.1.5",
    "@angular/compiler-cli": "16.1.5",
    "@angular/core": "16.1.5",
    "@angular/forms": "16.1.5",
    "@angular/material": "16.1.5",
    "@angular/platform-browser": "16.1.5",
    "@angular/platform-browser-dynamic": "16.1.5",
    "@angular/router": "16.1.5",
    "@bjanderson/utils": "2.1.2",
    "@types/jasmine": "4.3.5",
    "@types/jest": "29.5.3",
    "@types/node": "20.4.2",
    "@typescript-eslint/eslint-plugin": "5.45.1",
    "@typescript-eslint/eslint-plugin-tslint": "5.45.1",
    "@typescript-eslint/parser": "5.45.1",
    "core-js": "3.31.1",
    "cpx": "1.5.0",
    "eslint": "8.45.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jasmine": "4.1.3",
    "eslint-plugin-jsdoc": "46.4.3",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-rxjs": "5.0.3",
    "jasmine-core": "5.0.1",
    "jest": "29.6.1",
    "jest-cli": "29.6.1",
    "jest-preset-angular": "13.1.1",
    "ng-packagr": "16.1.0",
    "prettier": "3.0.0",
    "prettier-eslint": "15.0.1",
    "rimraf": "5.0.1",
    "rxjs": "7.8.1",
    "ts-jest": "29.1.1",
